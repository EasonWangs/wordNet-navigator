{
  "name": "wordnet-navigator",
  "version": "1.0.0",
  "description": "Modern WordNet visualization tool with interactive graph navigation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.ts --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{vue,ts,css}\"",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "cytoscape": "^3.28.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.21.4",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^6.3.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.6"
  },
  "keywords": [
    "wordnet",
    "visualization",
    "graph",
    "nlp",
    "vue",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
